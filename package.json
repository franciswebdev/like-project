    "@codegenie/serverless-express": "^4.15.0",
    "@nestjs/common": "^10.4.3",
    "@nestjs/core": "^10.4.3",
    "aws-lambda": "^1.0.7",
    "express": "^4.19.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/platform-express": "^10.4.3",
    "eslint": "^9.10.0",
    "typescript": "^5.6.2"
  }
}
